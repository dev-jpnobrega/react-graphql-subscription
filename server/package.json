{
  "name": "graphql-",
  "version": "1.0.0",
  "description": "Boilertemplate Graphql",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "yarn run lint && yarn run transpile",
    "transpile": "babel src --quiet --out-dir build --ignore 'src/**/*.spec.js'",
    "server": "node build/index.js",
    "test": "jest --config test.json --forceExit",
    "test:integration": "jest --config test.json --forceExit --collectCoverage false src/__tests__/integration/",
    "test:unit": "jest --config test.json --forceExit --testPathIgnorePatterns src/__tests__/ --collectCoverage true",
    "lint": "eslint .\\src"
  },
  "author": "dev-jpnobrega",
  "license": "ISC",
  "dependencies": {
    "apollo-link-http": "^1.4.0",
    "apollo-server-express": "^1.3.2",
    "apollo-server-lambda": "^2.14.2",
    "body-parser": "^1.18.2",
    "bookshelf": "^0.12.1",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.21.0",
    "http": "^0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.1.2",
    "knex": "^0.14.3",
    "lodash": "^4.17.4",
    "pg": "^7.4.1",
    "request": "^2.83.0",
    "subscriptions-transport-ws": "^0.9.9",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.1",
    "fs-extra": "^5.0.0",
    "jest": "^22.3.0",
    "jest-junit": "^3.6.0",
    "kill-port": "^1.5.1",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2"
  }
}
